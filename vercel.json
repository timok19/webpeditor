{
  "builds": [
    {
      "src": "webpeditor/wsgi.py",
      "use": "@ardnt/vercel-python-wsgi",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "output": ".vercel"
      }
    }
  ],
  "outputDirectory": ".vercel",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "webpeditor/wsgi.py"
    }
  ]
}